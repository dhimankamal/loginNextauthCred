// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(cuid())
  password String
  userid   String  @unique
  name     String?
  email    String? @unique
  gender  String
  dob   DateTime
  placeOfBirth String
  age Int
  fatherName String
  motherName String
  address String
  pincode Int
  district String
  maritalStatus String
  spouseName String
  phoneNumber Int
  voterId Int
  adharId Int
}

model Services {
  id     String  @id @default(cuid())
  title  String
  text   String
}
